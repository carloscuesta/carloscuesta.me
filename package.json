    "lodash.truncate": "4.4.2",
    "next": "16.0.0",
    "next-pwa": "^5.6.0",
    "next-seo": "^6.8.0",
    "next-themes": "0.4.6",
    "plaiceholder": "2.5.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-remove-scroll": "2.7.1",
    "rehype-autolink-headings": "7.1.0",
    "rehype-external-links": "^3.0.0",
    "rehype-preset-minify": "7.0.1",
    "rehype-pretty-code": "0.14.1",
    "rehype-probe-image-size": "1.0.0",
