    "src/**/*.{css,md,mdx}": [
      "prettier --write"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "packageManager": "pnpm@10.13.1",
  "pnpm": {
    "overrides": {
      "@types/react": "19.2.2"
    }
  }
